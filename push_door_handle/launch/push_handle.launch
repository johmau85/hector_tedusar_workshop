<launch>

  <arg name="debug" default="false" />
  <arg unless="$(arg debug)" name="launch_prefix" value="" />
  <arg     if="$(arg debug)" name="launch_prefix" value="gdb --ex run --args" />

    

    <node name="tug_push_door_handle_node" pkg="tug_push_door_handle" type="push_door_handle_node" >
    <rosparam command="load" file="$(find baxter_moveit_config)/config/kinematics.yaml"/>
   <remap from="/joint_states" to="/robot/joint_states" />
</node>

  <!-- Launch grasp handle -->
  <node name="push_handle"  pkg="push_door_handle" 
	type="push_handle" output="screen">
   <rosparam command="load" file="$(find baxter_moveit_config)/config/kinematics.yaml"/>
   <remap from="/joint_states" to="/robot/joint_states" />
  </node>

 



</launch>
